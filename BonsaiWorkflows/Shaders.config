<?xml version="1.0" encoding="utf-8"?>
<ShaderWindowSettings xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Width>1280</Width>
  <Height>800</Height>
  <VSync>On</VSync>
  <ClearColor>White</ClearColor>
  <ClearMask>DepthBufferBit ColorBufferBit</ClearMask>
  <WindowState>Fullscreen</WindowState>
  <DisplayDevice>Second</DisplayDevice>
  <TargetRenderFrequency>60</TargetRenderFrequency>
  <RenderState>
    <StateConfiguration xsi:type="EnableState">
      <Capability>PointSprite</Capability>
    </StateConfiguration>
    <StateConfiguration xsi:type="EnableState">
      <Capability>ProgramPointSize</Capability>
    </StateConfiguration>
  </RenderState>
  <GraphicsMode>
    <ColorFormat>
      <Red>8</Red>
      <Green>8</Green>
      <Blue>8</Blue>
      <Alpha>8</Alpha>
    </ColorFormat>
    <Depth>16</Depth>
    <Stencil>0</Stencil>
    <Samples>0</Samples>
    <AccumulatorFormat>
      <Red>0</Red>
      <Green>0</Green>
      <Blue>0</Blue>
      <Alpha>0</Alpha>
    </AccumulatorFormat>
    <Buffers>2</Buffers>
    <Stereo>false</Stereo>
  </GraphicsMode>
  <Shaders>
    <ShaderConfiguration xsi:type="Material">
      <Name>bg</Name>
      <Enabled>true</Enabled>
      <RenderState />
      <ShaderUniforms />
      <BufferBindings />
      <FramebufferAttachments />
      <VertexShader>bg.vert</VertexShader>
      <FragmentShader>bg.frag</FragmentShader>
      <MeshName>bg</MeshName>
    </ShaderConfiguration>
    <ShaderConfiguration xsi:type="Material">
      <Name>Particles</Name>
      <Enabled>true</Enabled>
      <RenderState />
      <ShaderUniforms />
      <BufferBindings />
      <FramebufferAttachments />
      <VertexShader>particles.vert</VertexShader>
      <FragmentShader>particles.frag</FragmentShader>
      <MeshName />
    </ShaderConfiguration>
    <ShaderConfiguration xsi:type="Material">
      <Name>Waveform</Name>
      <Enabled>true</Enabled>
      <RenderState />
      <ShaderUniforms />
      <BufferBindings />
      <FramebufferAttachments />
      <VertexShader>waveformSelf.vert</VertexShader>
      <GeometryShader>waveform_self.geom</GeometryShader>
      <FragmentShader>waveformSelf.frag</FragmentShader>
    </ShaderConfiguration>
  </Shaders>
  <Textures />
  <Meshes>
    <MeshConfiguration xsi:type="TexturedQuad">
      <Name>bg</Name>
      <QuadEffects>None</QuadEffects>
    </MeshConfiguration>
    <MeshConfiguration xsi:type="TexturedQuad">
      <Name>Waveform</Name>
      <QuadEffects>None</QuadEffects>
    </MeshConfiguration>
  </Meshes>
</ShaderWindowSettings>